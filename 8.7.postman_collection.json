	"item": [
		{
			"name": "Get Api Key",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"проверка статус-кода\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"запись полученного API-ключа в переменную auth_key\", function () {\r",
							"    var jsonData = JSON.parse(responseBody);\r",
							"    pm.expect(jsonData).to.have.all.key('key') \r",
							"    pm.collectionVariables.set(\"auth_key\", jsonData.key);\r",
							"});    "
						],
						"type": "text/javascript"
					}
				}
			],
			"protocolProfileBehavior": {
				"disableBodyPruning": true
			},
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "email",
						"value": "testjjj@test.test",
						"type": "text"
					},
					{
						"key": "password",
						"value": "123123",
						"type": "text"
					}
				],
				"body": {
					"mode": "formdata",
					"formdata": []
				},
				"url": {
					"raw": "{{base_url}}/api/key",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"api",
						"key"
					]
				}
			},
			"response": []
		},
		{
			"name": "Add Pet",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"проверка статус-кода\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"добавление ID питомца из ответа в переменную petid коллекции\", function () {\r",
							"   var jsonData = JSON.parse(responseBody);\r",
							"    pm.expect(jsonData).to.include.keys('id');\r",
							"    pm.collectionVariables.set(\"petid\", jsonData.id);\r",
							"});\r",
							"\r",
							"pm.test(\"запись name питомца из ответа в переменную name коллекции\", function () {\r",
							"   var jsonData = JSON.parse(responseBody);\r",
							"    pm.expect(jsonData).to.include.keys('name');\r",
							"    pm.collectionVariables.set(\"name\", jsonData.name);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "formdata",
					"formdata": [
						{
							"key": "name",
							"value": "{{name}}",
							"type": "text"
						},
						{
							"key": "animal_type",
							"value": "{{type}}",
							"type": "text"
						},
						{
							"key": "age",
							"value": "{{age}}",
							"type": "text"
						}
					]
				},
				"url": {
					"raw": "{{base_url}}/api/create_pet_simple",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"api",
						"create_pet_simple"
					]
				}
			},
			"response": []
		},
		{
			"name": "update pet",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"новое имя, полученное в ответе, не равно старому имени\", function () {\r",
							"   var jsonData = JSON.parse(responseBody);\r",
							"   pm.expect(pm.collectionVariables.get(\"name\")).to.not.equal(jsonData.name);\r",
							"});\r",
							"\r",
							"pm.test(\"запись нового имени сохранить в переменную name коллекции\", function () {\r",
							"   var jsonData = JSON.parse(responseBody)\r",
							"   pm.collectionVariables.set(\"name\",jsonData.name);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "PUT",
				"header": [],
				"body": {
					"mode": "formdata",
					"formdata": [
						{
							"key": "age",
							"value": "{{age}}",
							"type": "text"
						},
						{
							"key": "name",
							"value": "{{name2}}",
							"type": "text"
						}
					]
				},
				"url": {
					"raw": "{{base_url}}/api/pets/{{petid}}",
					"host": [
